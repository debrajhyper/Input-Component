{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "test/dist",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "test/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/test/$1"
        }
    ]
}